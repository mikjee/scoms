{
	"name": "scoms",
	"version": "1.0.0",
	"main": "src/monolith/app.ts",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"pgtyped:watch": "npx pgtyped --watch --config ./pgtyped.config.dev.js",
		"pgtyped:generate:dev": "npx pgtyped --config ./pgtyped.config.dev.js",
		"pgtyped:generate:prod": "npx pgtyped --config ./pgtyped.config.prod.js",
		"dev:watch-pg": "npm-run-all --parallel dev:pgtyped:watch dev",
		"dev": "ts-node-dev --respawn src/monolith/app.ts",
		"build": "tsc",
		"start-monolith": "node dist/monolith/app.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mikjee/scoms.git"
	},
	"author": "mikjee",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/mikjee/scoms/issues"
	},
	"homepage": "https://github.com/mikjee/scoms#readme",
	"description": "",
	"dependencies": {
		"@pgtyped/runtime": "^2.4.2",
		"pg": "^8.14.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.23.0",
		"@pgtyped/cli": "^2.4.3",
		"@types/node": "^22.13.13",
		"dotenv": "^16.4.7",
		"eslint": "^9.23.0",
		"eslint-plugin-boundaries": "^5.0.1",
		"globals": "^16.0.0",
		"npm-run-all": "^4.1.5",
		"pgtyped": "^0.0.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.27.0"
	}
}
